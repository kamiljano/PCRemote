package rmi.communication;

import java.io.Serializable;

public class ClientInfo implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -5696441797336285190L;
	
	private String os;
	private String osVer;
	private String osArch;
	private String user;
	private String mac;
	private String hostname;
	private String ip;
	
	public ClientInfo (String os, String osVer, String osArch, String user, String hostname, String ip, String mac)
	{
		this.os = os;
		this.osVer = osVer;
		this.osArch = osArch;
		this.user = user;
		this.mac = mac;
		this.ip = ip;
		this.hostname = hostname;
	}
	
	public String getOS()
	{
		return os;
	}
	public String getOSVer()
	{
		return osVer;
	}
	public String getOSArch()
	{
		return osArch;
	}
	public String getUser()
	{
		return user;
	}
	public String getMac()
	{
		return mac;
	}
	public String getHostname()
	{
		return hostname;
	}
	public String getIP()
	{
		return ip;
	}
}
